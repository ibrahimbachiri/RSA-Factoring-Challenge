import math

def is_prime(num):
    if num < 2:
        return False
    for i in range(2, int(math.sqrt(num)) + 1):
        if num % i == 0:
            return False
    return True

def find_prime_factors(n):
    for p in range(2, n + 1):
        if is_prime(p) and n % p == 0:
            q = n // p
            if is_prime(q):
                return p, q

if __name__ == "__main__":
    import sys

    if len(sys.argv) != 2:
        print("Usage: python rsa.py <number>")
        sys.exit(1)

    try:
        n = int(sys.argv[1])
    except ValueError:
        print("Invalid input. Please provide an integer.")
        sys.exit(1)

    if n < 2:
        print("Invalid input. Please provide a positive integer greater than 1.")
        sys.exit(1)

    p, q = find_prime_factors(n)

    if p is None or q is None:
        print(f"No prime factors found for {n}")
    else:
        print(f"{n} = {p}*{q}")
